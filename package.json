{
  "name": "lsc-token",
  "version": "1.0.0",
  "scripts": {
    "clean": "npx hardhat clean",
    "test": "npx hardhat test",
    "size": "hardhat size-contracts",
    "compile": "npx hardhat compile",
    "prepend-spdx": "hardhat prepend-spdx-license",
    "deploy-bsc-testnet": "yarn hardhat run --network bscTestnet scripts/deploy.ts",
    "deploy-bsc": "npx hardhat run --network bsc scripts/deploy.ts",
    "precommit": "lint-staged",
    "generate:docs": "hardhat docgen",
    "lint": "prettier '**/*.{json,js,ts,sol}' --check && solhint 'contracts/*.sol' && eslint '**/*.{js,ts}'",
    "format": "prettier '**/*.{json,js,ts,sol}' --write && solhint 'contracts/*.sol' --fix && eslint '**/*.{js,ts}' --fix"
  },
  "dependencies": {
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@openzeppelin/contracts": "^4.8.3",
    "@openzeppelin/contracts-upgradeable": "^4.8.3",
    "dotenv": "^16.0.3",
    "ethers": "5.7.2",
    "hardhat": "^2.14.0",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.6",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@openzeppelin/hardhat-upgrades": "^1.24.0",
    "@typechain/ethers-v5": "^10.2.0",
    "@typechain/hardhat": "^6.1.5",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "chai": "^4.2.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "hardhat-abi-exporter": "^2.10.1",
    "hardhat-contract-sizer": "^2.8.0",
    "hardhat-docgen": "^1.3.0",
    "hardhat-gas-reporter": "^1.0.9",
    "hardhat-spdx-license-identifier": "^2.1.0",
    "hardhat-tracer": "^2.2.2",
    "prettier": "^2.8.8",
    "prettier-plugin-solidity": "^1.1.3",
    "solhint": "^3.4.1",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.8.2",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.0"
  }
}